{
  "name": "@nywqs/vue-markdown-editor",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --mode lib && npm run build:types",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist-lib",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "license": "MIT",
  "dependencies": {
    "marked": "^17.0.1",
    "highlight.js": "^11.10.0",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@vicons/ionicons5": "^0.13.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vue-tsc": "^3.2.2"
  },
  "main": "dist-lib/vue-markdown-editor.umd.js",
  "module": "dist-lib/vue-markdown-editor.es.js",
  "style": "dist-lib/wqs_editor.css",
  "files": [
    "dist-lib"
  ],
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "types": "dist-lib/src/lib/index.d.ts"
}
